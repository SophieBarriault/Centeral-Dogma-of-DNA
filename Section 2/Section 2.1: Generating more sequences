import random
import numpy as np
import pandas as pd
from random import randint
import string 

#Generate some random DNA sequences to work with: 

num_seq = random.randint(1, 8)
DNA_dict = {}
for i in range(num_seq):
  seq_id = f"sequence_{i+1}:"
  seq_length = random.randint(10, 100)
  my_seq = ''.join(np.random.choice(('C','G','T','A'), seq_length ))
  DNA_dict[seq_id] = my_seq

print("My sequence:", DNA_dict) 
